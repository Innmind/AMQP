{
    "name": "innmind/amqp",
    "type": "library",
    "description": "AMQP client",
    "keywords": ["amqp"],
    "homepage": "http://github.com/Innmind/AMQP",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/AMQP/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~5.2",
        "innmind/time-continuum": "~3.1",
        "innmind/math": "~6.0",
        "innmind/url": "~4.1",
        "ramsey/uuid": "~4.0",
        "innmind/operating-system": "~4.1",
        "innmind/media-type": "~2.0",
        "innmind/filesystem": "~7.0",
        "innmind/stream": "~4.0",
        "innmind/io": "^2.4.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\AMQP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\AMQP\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "psr/log": "~3.0",
        "vimeo/psalm": "~5.15",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0"
    },
    "suggest": {
        "psr/log": "To log what's happening through the connection"
    }
}
